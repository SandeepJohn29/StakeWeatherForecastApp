{
  "expo": {
    "name": "StakeWeatherForecastApp",
    "slug": "StakeWeatherForecastApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon2.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.anonymous.StakeWeatherForecastApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon2.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.anonymous.StakeWeatherForecastApp"
    },
    "web": {
      "favicon": "./assets/icon2.png"
    },
    "plugins": [
      "@react-native-firebase/app"
    ]
  }
}
